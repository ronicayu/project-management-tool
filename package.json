{
  "name": "teu-project-management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev --prefix server\"",
    "build": "vite build",
    "preview": "vite preview",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down",
    "db:init": "docker-compose exec -T postgres psql -U teu -d teu < server/schema.sql"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "vite": "^5.2.0",
    "typescript": "^5.4.0"
  }
}
